// dynamically generate google map urls using to and from lat/long numbersfunction createGmapURL(nFromLat:Number, nFromLong:Number, nToLat:Number, nToLong:Number):String {	var sURLBase:String = "http://maps.google.com/maps?f=q&hl=en&q=";		var sFinalURL = sURLBase + nFromLat.toString() + ",+"; 	sFinalURL += nFromLong.toString() + "+to+";	sFinalURL += nToLat.toString() + ",+";	sFinalURL += nToLong.toString();		return sFinalURL;}